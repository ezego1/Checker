{
  "name": "smart-check",
  "version": "1.0.0",
  "description": "health check app",
  "main": "dist/src/index.js",
  "author": "seamfix",
  "license": "MIT",
  "scripts": {
    "watch": "tsc --watch",
    "client": "yarn --cwd client start",
    "cleanup": "rimraf ./dist",
    "build": "rimraf ./dist && tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:quick": "node dist/src/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "work": "concurrently \"yarn dev\" \"yarn client\"",
    "build:prod": "concurrently \"yarn start:quick\" \"yarn client\" ",
    "start:prod": "bash build.sh"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jw-paginate": "^1.0.4",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.29",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
